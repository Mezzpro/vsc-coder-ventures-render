{
  "workbench.colorTheme": "Default Dark+",
  "workbench.startupEditor": "readme",
  "workbench.colorCustomizations": {
    "editor.background": "#0a0e1a",
    "activityBar.background": "#0a0e1a",
    "sideBar.background": "#0f1419",
    "statusBar.background": "#1a1f3a",
    "statusBar.foreground": "#64ffda",
    "editor.foreground": "#a9b7c6",
    "editorCursor.foreground": "#64ffda",
    "terminal.background": "#0a0e1a",
    "panel.background": "#0f1419",
    "titleBar.activeBackground": "#1a1f3a",
    "titleBar.activeForeground": "#64ffda"
  },
  "editor.fontFamily": "monospace",
  "editor.fontSize": 14,
  "editor.lineHeight": 1.6,
  "editor.tabSize": 2,
  "editor.insertSpaces": true,
  "window.title": "MezzPro Blockchain Platform - ${folderName}",
  "git.autofetch": true,
  "files.autoSave": "onFocusChange",
  "terminal.integrated.fontSize": 13,
  "workbench.iconTheme": "vs-seti",
  "breadcrumbs.enabled": true,
  "editor.minimap.enabled": true
}